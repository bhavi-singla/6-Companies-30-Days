Decode the string
Solution:-
string modify(string s,int &i,int n)
    {
        string str="";
        stack<char>st;
        st.push(s[i]);
        i++;
        while(i<n && !st.empty())
        {
            if(s[i]==']')
            {
                st.pop();
                i++;
            }
            else if(s[i]>=97 && s[i]<=122)
            {
                str+=s[i];
                i++;
            }
            else if(s[i]>=48 && s[i]<=57)
            {
                int num=0;
                while(s[i]>=48 && s[i]<=57)
                {
                    num=num*10+s[i]-48;
                    i++;
                }
                string temp=modify(s,i,n);
                for(int i=0;i<num;i++)
                {
                    str+=temp;
                }
            }
        }
        return str;
    }
    string decodeString(string s) {
        int n=s.length();
        string str="";
        int i=0;
        int num=0;
        while(i<n)
        {
            if(s[i]>=97 && s[i]<=122)
            {
                str+=s[i];
                i++;
            }
            else if(s[i]>=48 && s[i]<=57)
            {
                int num=0;
                while(s[i]>=48 && s[i]<=57)
                {
                    num=num*10+s[i]-48;
                    i++;
                }
                string temp=modify(s,i,n);
                for(int i=0;i<num;i++)
                {
                    str+=temp;
                }
            }
        }
        return str;
    }
