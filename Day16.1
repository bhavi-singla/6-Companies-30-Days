Find a continuous sub-array which adds to a given number S.

vector<int> subarraySum(int arr[], int n, long long s)
    {
        int start=0;
        vector<int>vec;
        int end=0;
        long long int sum=0;
        while(end<n)
        {
            while(sum<s && end<n)
            {
                sum+=arr[end];
                end++;
            }
            
            while(sum>s && start<end)
            {
                sum-=arr[start];
                start++;
            }
            if(sum==s)
            {
                vec.push_back(start+1);
                vec.push_back(end);
                //cout<<"bhavi"<<endl;
                break;
            }
            //cout<<start<<" "<<sum<<endl;
        }
        if(vec.empty())
            vec.push_back(-1);
        return vec;    
    }
