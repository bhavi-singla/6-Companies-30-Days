Rotate Matrix by 90deg anticlockwise

void rotate(int n,int a[][n])
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<i;j++)
        {
            int temp=a[i][j];
            a[i][j]=a[j][i];
            a[j][i]=temp;
        }
    }
    for(int i=0;i<n/2;i++)
    {
        for(int j=0;j<n;j++)
        {
            int temp=a[i][j];
            a[i][j]=a[n-i-1][j];
            a[n-i-1][j]=temp;
        }
    }
}
