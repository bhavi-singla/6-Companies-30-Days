Atoi

int atoi(string str) {
        int sign=1;
        int i=0;
        if(str[i]=='-')
        {
            sign=-1;
            i++;
        }
        int num=0;
        for(i;i<str.length();i++)
        {
            if(str[i]>=48 && str[i]<=57)
            {
                num=num*10+str[i]-'0';
            }
            else return -1;
        }
        return num*sign;
    }
