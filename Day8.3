Which among them forms a perfect Sudoku Pattern ?
Solution:-
int isValid(vector<vector<int>> mat){
        for(int i=0;i<9;i++)
        {
            int mprow[10];
            memset(mprow,0,sizeof(mprow));
            int mpcol[10];
            memset(mpcol,0,sizeof(mpcol));
            for(int j=0;j<9;j++)
            {
                if(mat[i][j]!=0)
                {
                    if(mprow[mat[i][j]]<1)
                    {
                        mprow[mat[i][j]]++;
                    }
                    else
                    {
                        //cout<<i<<" "<<j<<endl;
                        return 0;
                    }
                }
                if(mat[j][i]!=0)  
                {
                    if(mpcol[mat[j][i]]<1)
                    {
                        mpcol[mat[j][i]]++;
                    }
                    else
                    {
                        //<<i<<" "<<j<<endl;
                        return 0;
                    }
                }
                
            }
        }
        for(int i=0;i<9;i+=3)
        {
            for(int j=0;j<9;j+=3)
            {
                vector<int>count(10,0);
                for(int k=i;k<i+3;k++)
                {
                    for(int l=j;l<j+3;l++)
                    {
                        if(count[mat[k][l]]>=1)
                        {
                            return 0;
                        }
                        if(mat[k][l]!=0)
                        count[mat[k][l]]++;
                    }
                }
            }
        }
        return 1;
    }
