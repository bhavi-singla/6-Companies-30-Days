Total Decoding Messages 
Solution:-
long long int dp[10001];
	    int findtotal(string str,long long int count,int i,int n)
	    {
	        dp[n]=1;
	        dp[n-1]=1;
	        if(str[n-1]=='0')
	            dp[n-1]=0;
	        for(int i=n-2;i>=0;i--)
	        {
	            if(str[i]=='0')
	                dp[i]=0;
	            else{
	                dp[i]=dp[i+1];
	                dp[i]=dp[i]%count;
	                if(str[i]<'2' || (str[i]=='2' && str[i+1]<'7'))
	                {
	                    dp[i]+=dp[i+2];
	                    dp[i]=dp[i]%count;
	                }
	            }
	            
	        }
	        return dp[0];
	    }
		int CountWays(string str){
		    long long int count=1000000007;
		    
		    memset(dp,0,sizeof(dp));
		    return findtotal(str,count,0,str.length());
		}
