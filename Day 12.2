Stock span problem

#include <stack>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int price[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>price[i];
	    }
	    stack<int>s;
        int vec[n];
    vec[0]=1;
    s.push(0);
    for(int i=1;i<n;i++)
    {
        while( (!s.empty()) && (price[s.top()]<=price[i]) )
        {
            s.pop();
        }
        
        if(s.empty())
            vec[i]=i+1;
        else
            vec[i]=i-s.top();
        
        s.push(i);
	}
	for(int j=0;j<n;j++)
	{
	    cout<<vec[j]<<" ";
	}
	cout<<endl;
	}
	return 0;
}
