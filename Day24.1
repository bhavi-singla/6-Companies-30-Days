    void dfs(vector<vector<int>>con,int i,vector<bool>& visited,int n)
    {
        visited[i]=true;
        for(int j=0;j<n;j++)
        {
            if(con[i][j] && !visited[j])
            {
                dfs(con,j,visited,n);
            }
        }
    }
    int findCircleNum(vector<vector<int>>& isConnected) {
        
        int n = isConnected.size();
        vector<bool>visited(n,false);
        int count=0;
        for(int i=0;i<n;i++)
        {
            if(!visited[i])
            {
                count++;
                dfs(isConnected,i,visited,n);
            }
        }
        return count;
    }
